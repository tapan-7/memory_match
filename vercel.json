{
    "routes": [
        {
            "src": "^/$",
            "dest": "/index.html"
        },
        {
            "src": "^/game$",
            "dest": "/game.html"
        },
        {
            "src": "^/explore$",
            "dest": "/explore.html"
        },
        {
            "src": "^/victory$",
            "dest": "/victory.html"
        },
        {
            "src": "^/modal$",
            "dest": "/modal.html"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": ".*",
            "dest": "/+not-found.html"
        }
    ]
}